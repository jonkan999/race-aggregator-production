{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "source": "functions",
    "codebase": "api-functions"
  },
  "hosting": [
    {
      "target": "se",
      "public": "build/se",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "/log-analytics",
          "function": "logAnalytics"
        }
      ]
    },
    {
      "target": "no",
      "public": "build/no",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "/log-analytics",
          "function": "logAnalytics"
        }
      ]
    }
  ]
}
